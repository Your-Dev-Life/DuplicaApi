{
  "name": "duplica-api",
  "version": "1.0.0",
  "description": "Duplica API",
  "main": "index.js",
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "unmockedModulePathPatterns": [
      "./node_modules/lodash",
      "./node_modules/mongoose"
    ]
  },
  "scripts": {
    "lint": "eslint server/** tests/**",
    "test": "jest --detectOpenHandles --forceExit --runInBand server",
    "test:auto": "jest --detectOpenHandles --watch --runInBand --forceExit server",
    "coverage": "jest --collectCoverageFrom=server/**/*.js --collectCoverageFrom=!server/utils/logger.js --collectCoverageFrom=!server/middleware/appMiddleware.js --coverage --runInBand --forceExit server",
    "start": "node index.js",
    "start:dev": "nodemon"
  },
  "author": "Marcel Silva",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-acl": "^2.0.8",
    "express-jwt": "^5.3.3",
    "express-validator": "^6.4.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.14",
    "morgan": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.0.1",
    "mockingoose": "^2.13.2",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2"
  }
}
